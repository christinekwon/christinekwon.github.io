        "three-obj-loader": "^1.1.3",
        "webpack-cli": "^3.3.12"
    },
    "devDependencies": {
        "@babel/core": "^7.21.0",
        "@babel/preset-env": "^7.20.2",
        "babel-loader": "^8.3.0",
        "css-loader": "^6.7.3",
        "file-loader": "^6.2.0",
        "gh-pages": "^2.2.0",
        "html-loader": "^4.2.0",
        "html-webpack-plugin": "^4.5.2",
        "mkdirp": "^1.0.4",
        "prettier": "^2.8.4",
        "raw-loader": "^4.0.2",
        "rimraf": "^3.0.2",
        "url-loader": "^4.1.1",
        "webpack": "^5.75.0",
